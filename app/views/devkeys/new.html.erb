
<% provide(:title, 'Register your Dev Key') %>
<h1>Register your Dev Key</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@devkey) do |f| %>
        <%= f.label :domain %>
        <%= f.text_field :domain %>

        <%= f.label :client_id %>
        <%= f.text_field :client_id %>

        <%= f.label :key %>
        <%= f.text_field :key %>

        <%= f.label :uri, "Redirect URI" %>
        <%= f.text_field :uri %>

        <%= f.submit "Register", class: "btn btn-primary" %>
    <% end %>
  </div>
